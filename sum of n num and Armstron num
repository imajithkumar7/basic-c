#include<stdio.h>
int main()
{
    int i,sum=0;
    for(i=1;i<=5;i++){
        sum=sum+i;
    }
    printf("sum is %d",sum);
}

#include<stdio.h>
int main(){
    int n,r,cube,sum=0,temp;
    printf("Enter n:");
    scanf("%d",&n);
    temp=n;
    while(n>0){
        r=n%10;
        cube=r*r*r;
        sum=sum+cube;
        n=n/10;
    }
    n=temp;
    if(n==sum)
    {
        printf("Arm num");
    }
    else{
        printf("NO");
    }
}
